<h1>Hello, world!</h1>
<table class="table">
  <tr>
    <th>Nama</th>
    <th>NIM</th>
    <th>Jurusan</th>
  </tr>
  <% if (mahasiswa.length < 1) { %>
  <p>Data Masih kosong</p>
  <% } else { %> <% mahasiswa.forEach(mhs => { %>
  <tr>
    <td><%= mhs.nama %></td>
    <td><%= mhs.nim %></td>
    <td><%= mhs.jurusan %></td>
  </tr>
  <% }) %> <% } %>
</table>
